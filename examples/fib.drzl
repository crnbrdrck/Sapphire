def fib(n: int) -> int {
    # Return the nth fibonacci number because this is always a benchmark /shrug
    if (n == 0) {
        return 0
    } elsif (n == 1) {
        return 1
    } else {
        return fib(n - 1) + fib(n - 2)
    }
}

fib(20)

